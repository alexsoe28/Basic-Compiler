text
@// $Id: 30-fac-fnloop.oc,v 1.2 2011-10-20 21:41:55-07 - - $
//
// Function uses a loop to compute factorial.
//

#include "oclib.oh"

int fac (int n) {
   int f = 1;
   while (n > 1) {
      f = f * n;
      n = n - 1;
   }
   return f;
}

int n = 1;
while (n <= 5) {
   puti (fac (n));
}

@


