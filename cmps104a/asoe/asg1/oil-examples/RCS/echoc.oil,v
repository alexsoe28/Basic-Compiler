#include "oclib.oh"

ubyte **__argv;
int __argi;

void __ocmain ()
{
	ubyte **p1 = __getargv ();
	__argv = p1;
	__argi = 1;
while_5_7_0:;
	ubyte *p2 = __argv[__argi];
	ubyte b3 = p2 != 0;
	if (!b3) goto break_5_7_0;
	ubyte b4 = __argi > 1;
	if (!b4) goto fi_5_8_3;
	__putc (' ');
fi_5_8_3:;
